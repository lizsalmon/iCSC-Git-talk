#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/gfk62481/CSC/iCSC/iCSC-Git-talk/node_modules/.pnpm/@slidev+cli@52.11.5_@nuxt+kit@3.21.0_@types+markdown-it@14.1.2_@types+node@22.19.8_@vue_8912303b1108713e4d158becfcec3df7/node_modules/@slidev/cli/bin/node_modules:/home/gfk62481/CSC/iCSC/iCSC-Git-talk/node_modules/.pnpm/@slidev+cli@52.11.5_@nuxt+kit@3.21.0_@types+markdown-it@14.1.2_@types+node@22.19.8_@vue_8912303b1108713e4d158becfcec3df7/node_modules/@slidev/cli/node_modules:/home/gfk62481/CSC/iCSC/iCSC-Git-talk/node_modules/.pnpm/@slidev+cli@52.11.5_@nuxt+kit@3.21.0_@types+markdown-it@14.1.2_@types+node@22.19.8_@vue_8912303b1108713e4d158becfcec3df7/node_modules/@slidev/node_modules:/home/gfk62481/CSC/iCSC/iCSC-Git-talk/node_modules/.pnpm/@slidev+cli@52.11.5_@nuxt+kit@3.21.0_@types+markdown-it@14.1.2_@types+node@22.19.8_@vue_8912303b1108713e4d158becfcec3df7/node_modules:/home/gfk62481/CSC/iCSC/iCSC-Git-talk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/gfk62481/CSC/iCSC/iCSC-Git-talk/node_modules/.pnpm/@slidev+cli@52.11.5_@nuxt+kit@3.21.0_@types+markdown-it@14.1.2_@types+node@22.19.8_@vue_8912303b1108713e4d158becfcec3df7/node_modules/@slidev/cli/bin/node_modules:/home/gfk62481/CSC/iCSC/iCSC-Git-talk/node_modules/.pnpm/@slidev+cli@52.11.5_@nuxt+kit@3.21.0_@types+markdown-it@14.1.2_@types+node@22.19.8_@vue_8912303b1108713e4d158becfcec3df7/node_modules/@slidev/cli/node_modules:/home/gfk62481/CSC/iCSC/iCSC-Git-talk/node_modules/.pnpm/@slidev+cli@52.11.5_@nuxt+kit@3.21.0_@types+markdown-it@14.1.2_@types+node@22.19.8_@vue_8912303b1108713e4d158becfcec3df7/node_modules/@slidev/node_modules:/home/gfk62481/CSC/iCSC/iCSC-Git-talk/node_modules/.pnpm/@slidev+cli@52.11.5_@nuxt+kit@3.21.0_@types+markdown-it@14.1.2_@types+node@22.19.8_@vue_8912303b1108713e4d158becfcec3df7/node_modules:/home/gfk62481/CSC/iCSC/iCSC-Git-talk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@slidev+cli@52.11.5_@nuxt+kit@3.21.0_@types+markdown-it@14.1.2_@types+node@22.19.8_@vue_8912303b1108713e4d158becfcec3df7/node_modules/@slidev/cli/bin/slidev.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/@slidev+cli@52.11.5_@nuxt+kit@3.21.0_@types+markdown-it@14.1.2_@types+node@22.19.8_@vue_8912303b1108713e4d158becfcec3df7/node_modules/@slidev/cli/bin/slidev.mjs" "$@"
fi
